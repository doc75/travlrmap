<% if Array(@map[:map_types]).include?("watercolor") %>
map.addMapType("watercolor", {
  getTileUrl: function(coord, zoom) {
    return "http://a.tile.stamen.com/watercolor/" + zoom + "/" + coord.x + "/" + coord.y + ".png";
  },
  tileSize: new google.maps.Size(256, 256),
  name: "Water Color",
  maxZoom: 18
});
<% end %>
